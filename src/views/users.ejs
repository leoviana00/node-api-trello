<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../views/head'); %>
  </head>
  <body class="container">
    <header><%- include('../views/header'); %></header>

    <main>
      <div class="container mt-3">
        <table id="test_table" class="table table-striped table-bordered" style="width: 100%" >
          <thead>
            <tr>
              <th>Nome</th>
              <th>To-Do</th>
              <th>Doing</th>
              <th>Terceiros</th>
              <th>Listar</th>
            </tr>
          </thead>

          <tbody id="ListSprint">
            <% users.forEach(function(user){ %>
            <tr>
              <td><%= user.username %></td>
              <td><%= user.todo.length %></td>
              <td><%= user.doing.length %></td>
              <td><%= user.terceiros.length %></td>
              <td>
                <div class="btn-group">
                  <a class="btn btn-primary" href="https://trello.com/b/ojEiqbeQ/infra-devops?menu=filter&filter=member:<%= user.username %>" target="_blank">Listar</a>
                </div>
          </td>
            </tr>
            <% }); %>

          </tbody>
        </table>
      </div>
    </main>

    <footer><%- include('../views/footer'); %></footer>
  </body>
</html>
