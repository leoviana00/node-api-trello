<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../views/head'); %>
</head>
<body class="container">

<header>
    <%- include('../views/header'); %>
</header>

<main>

    <table id="test_table" class="table table-striped table-bordered mt-3" style="width:100%">
        <thead>
            <tr>
                <th>Nome da Sprint</th>
                <th>Total de atividades</th>
                <th>Conclu√≠das</th>
                <th><i class="icon_cogs"></i> Action</th>
              
            </tr>
        </thead>
        <tbody id="ListSprint">
        <% list.forEach(function(idList){ %>
            <tr>
                
                <td><%= idList.name %></td>
                <td><%= idList.badges.checkItems %></td>
                <td><%= idList.badges.checkItemsChecked %></td>
                
                <td>
                    <div class="btn-group">
                      <a class="btn btn-primary" href="/tarefas?cardid=<%= idList.shortLink %>">Listar</a>
                    </div>
              </td>
                
                
                
                
            </tr>
        <% }); %>
        </tbody>    
      </table>

                  

</main>

<footer>
    <%- include('../views/footer'); %>
</footer>

</body>
</html>
